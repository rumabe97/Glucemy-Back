{
    "builds": [{
        "src": "config/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "build": {
        "scripts": {
            "postBuild": "python manage.py migrate",
            "postBuild": "python manage.py loaddata foods/fixtures/foods.json"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "config/wsgi.py"
        }
    ]
}